{
  "name": "smart-mirror",
  "version": "0.0.0",
  "description": "Get started with React, Redux, and React-Router!",
  "scripts": {
    "clean": "rm -rf dist",
    "start": "npm run dev -- --nw",
    "compile": "webpack -p",
    "copy-cname": "cp ./CNAME ./dist/CNAME",
    "dev": "node --harmony bin/webpack-dev-server",
    "dev:nw": "npm run dev -- --nw",
    "dev:no-debug": "npm run dev -- --no_debug",
    "deploy": "npm run clean && npm run compile -- --no_debug && npm run copy-cname && surge -p ./dist"
  },
  "dependencies": {
    "axios": "0.5.4",
    "babel": "5.8.23",
    "history": "1.9.1",
    "moment": "2.10.6",
    "react": "0.14.0-rc1",
    "react-dom": "0.14.0-rc1",
    "react-redux": "2.1.2",
    "react-router": "1.0.0-rc1",
    "redux": "3.0.0",
    "yargs": "3.25.0"
  },
  "devDependencies": {
    "babel-eslint": "4.1.3",
    "babel-loader": "5.3.2",
    "babel-plugin-react-transform": "1.0.5",
    "babel-runtime": "5.8.24",
    "eslint": "1.5.0",
    "eslint-config-airbnb": "0.0.8",
    "eslint-loader": "1.0.0",
    "eslint-plugin-react": "3.4.2",
    "html-webpack-plugin": "1.6.1",
    "react-transform-catch-errors": "1.0.0",
    "react-transform-hmr": "1.0.0",
    "redbox-react": "1.0.6",
    "redux-devtools": "2.1.2",
    "surge": "0.15.0",
    "webpack": "1.12.2",
    "webpack-dev-server": "1.11.0"
  }
}
